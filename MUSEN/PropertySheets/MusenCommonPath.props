<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(SolutionDir)$(Platform)\$(Configuration)\</OutDir>
    <IntDir>$(Platform)\$(Configuration)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(SolutionDir)..\..\ExternalLibraries\zlib\include;$(SolutionDir)..\..\ExternalLibraries\protobuf\include;$(SolutionDir)..\..\Databases\AgglomerateDatabase;$(SolutionDir)..\..\Databases\GeometriesDatabase;$(SolutionDir)..\..\Databases\MaterialsDatabase;$(SolutionDir)..\..\Databases\MaterialsDatabase\GeneratedFiles;$(SolutionDir)..\..\GeneralFunctions;$(SolutionDir)..\..\GeneralFunctions\MUSEN;$(SolutionDir)..\..\GeneralFunctions\SimResultsStorage;$(SolutionDir)..\Modules\BondsGenerator;$(SolutionDir)..\Modules\ContactAnalyzers;$(SolutionDir)..\Modules\FileManager;$(SolutionDir)..\Modules\Geometries;$(SolutionDir)..\Modules\ObjectsGenerator;$(SolutionDir)..\Modules\PackageGenerator;$(SolutionDir)..\Modules\ResultsAnalyzer;$(SolutionDir)..\Modules\ScriptInterface;$(SolutionDir)..\Modules\SimplifiedScene;$(SolutionDir)..\Modules\Simulator;$(SolutionDir)..\Modules\SystemStructure;$(SolutionDir);$(SolutionDir)..\BuildVersion;$(CUDA_PATH)\include</AdditionalIncludeDirectories>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
      <MultiProcessorCompilation>true</MultiProcessorCompilation>
      <AdditionalOptions>/w34062 %(AdditionalOptions)</AdditionalOptions>
      <SupportJustMyCode>true</SupportJustMyCode>
      <LanguageStandard>stdcpp17</LanguageStandard>
      <PreprocessorDefinitions>_SILENCE_CXX17_CODECVT_HEADER_DEPRECATION_WARNING;_SILENCE_CXX17_OLD_ALLOCATOR_MEMBERS_DEPRECATION_WARNING;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <CustomBuild>
      <Command>mkdir GeneratedFiles
$(SolutionDir)..\..\ExternalLibraries\protobuf\bin\protoc.exe --proto_path=$(ProjectDir) --proto_path=$(SolutionDir)..\..\ --proto_path=$(SolutionDir)..\..\Databases\MaterialsDatabase\ --cpp_out=GeneratedFiles $(ProjectDir)%(Identity)</Command>
    </CustomBuild>
    <CustomBuild>
      <Outputs>GeneratedFiles\%(Filename).pb.h;GeneratedFiles\%(Filename).pb.cc;%(Outputs)</Outputs>
    </CustomBuild>
    <Link />
    <CudaCompile>
      <CodeGeneration>compute_52,sm_52;compute_61,sm_61;compute_70,sm_70;compute_75,sm_75;compute_80,sm_80;compute_86,sm_86;</CodeGeneration>
      <TargetMachinePlatform>64</TargetMachinePlatform>
      <AdditionalOptions>-Wno-deprecated-gpu-targets %(AdditionalOptions)</AdditionalOptions>
    </CudaCompile>
    <CudaLink />
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>